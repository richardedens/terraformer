{
  "name": "terraformer",
  "version": "0.0.0",
  "scripts": {
    "start": "npm-run-all run:sass run:start-dev",
    "build": "npm-run-all run:sass run:build",
    "run:sass": "sass src/sass/style.scss src/public/stylesheets/style.css",
    "run:build": "node ./util/build.js",
    "run:lint": "tslint --project \"tsconfig.json\"",
    "run:start": "cross-env ENV_FILE=production node -r ./env -r module-alias/register ./dist/start.js",
    "run:start-dev": "nodemon --config \"./util/nodemon.json\"",
    "run:test": "nodemon --config \"./util/nodemon.test.json\""
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cookie-parser": "^1.4.4",
    "cross-env": "^5.2.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "module-alias": "^2.2.1",
    "morgan": "^1.9.1",
    "npm-run-all": "^4.1.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/find": "^0.2.1",
    "@types/jasmine": "^3.4.0",
    "@types/jsonfile": "^5.0.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.4",
    "@types/supertest": "^2.0.8",
    "find": "^0.3.0",
    "fs-extra": "^8.1.0",
    "jasmine": "^3.4.0",
    "jsonfile": "^5.0.0",
    "nodemon": "^1.19.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.3.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "^5.19.0",
    "typescript": "^3.6.2"
  }
}
